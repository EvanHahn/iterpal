{
  "env": {
    "es2021": true
  },
  "extends": "eslint:recommended",
  "plugins": ["markdown"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-new": "off",
    "no-unused-expressions": "off"
  }
}
