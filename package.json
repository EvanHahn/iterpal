{
  "name": "iterpal",
  "description": "A friendly collection of utilities for iterables",
  "version": "0.2.0",
  "license": "MIT",
  "homepage": "https://github.com/EvanHahn/iterpal",
  "type": "module",
  "keywords": [
    "iterator",
    "iterable",
    "generator",
    "collection",
    "lazy"
  ],
  "bugs": {
    "url": "https://github.com/EvanHahn/iterpal/issues",
    "email": "me@evanhahn.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/EvanHahn/iterpal.git"
  },
  "scripts": {
    "pretest": "npm run lint",
    "lint:readme:textlint": "textlint README.md",
    "lint:readme:eslint": "eslint --fix --config .eslintrc-markdown.json --ext md .",
    "lint:readme": "npm run lint:readme:textlint && npm run lint:readme:eslint",
    "lint:code": "eslint --fix .",
    "lint": "npm run lint:code && npm run lint:readme",
    "test": "ava"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "*.js"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "ava": "^3.14.0",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "sinon": "^9.2.2",
    "textlint": "^11.7.6",
    "textlint-rule-alex": "^3.0.0",
    "textlint-rule-en-capitalization": "^2.0.3",
    "textlint-rule-no-empty-section": "^1.1.0",
    "textlint-rule-spellchecker": "^2.2.1",
    "textlint-rule-stop-words": "^2.0.7"
  }
}
